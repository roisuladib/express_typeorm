{
   "name": "express-typeorm",
   "version": "1.0.0",
   "main": "index.js",
   "author": "Muhammad Roisul Adib <99470918+roisuladib@users.noreply.github.com>",
   "license": "MIT",
   "scripts": {
      "start": "ts-node-dev --respawn --transpile-only --exit-child src/app.ts",
      "build": "rm -rf build && node scripts/build.mjs",
      "typeorm": "typeorm-ts-node-commonjs",
      "migrate": "yarn typeorm migration:generate ./src/migrations/create-posts-table -d ./src/utils/data-source.ts",
      "db:push": "yarn typeorm migration:run -d src/utils/data-source.ts",
      "lint": "eslint src/** --ext .ts",
      "precommit": "lint-staged"
   },
   "dependencies": {
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.20.2",
      "config": "^3.3.9",
      "cookie-parser": "^1.4.6",
      "cors": "^2.8.5",
      "dotenv": "^16.1.4",
      "envalid": "^7.3.1",
      "express": "^4.18.2",
      "helmet": "^7.0.0",
      "html-to-text": "^9.0.5",
      "jsonwebtoken": "^9.0.0",
      "nodemailer": "^6.9.3",
      "pg": "^8.11.0",
      "pug": "^3.0.2",
      "redis": "^4.6.7",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.16",
      "zod": "^3.21.4"
   },
   "devDependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/body-parser": "^1.19.2",
      "@types/config": "^3.3.0",
      "@types/cookie-parser": "^1.4.3",
      "@types/cors": "^2.8.13",
      "@types/express": "^4.17.17",
      "@types/html-to-text": "^9.0.1",
      "@types/jsonwebtoken": "^9.0.2",
      "@types/morgan": "^1.9.4",
      "@types/node": "^16.11.10",
      "@types/nodemailer": "^6.4.8",
      "@types/pug": "^2.0.6",
      "@typescript-eslint/eslint-plugin": "^5.59.9",
      "@typescript-eslint/parser": "^5.59.9",
      "esbuild": "^0.18.6",
      "eslint": "^8.42.0",
      "husky": "^8.0.3",
      "lint-staged": "^13.2.2",
      "morgan": "^1.10.0",
      "prettier": "^2.8.8",
      "ts-node": "10.7.0",
      "ts-node-dev": "^2.0.0",
      "typescript": "^5.1.3"
   },
   "lint-staged": {
      "*.ts": "yarn lint"
   }
}
